<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="himesp7.cis2237.com.grocerybag" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewGroceriesActivity"
            android:label="@string/title_activity_view_groceries"
            android:theme="@style/AppCompatActivities" >
        </activity>
        <activity
            android:name=".AddEditGroceriesActivity"
            android:label="@string/title_activity_add_edit_groceries"
            android:theme="@style/AppCompatActivities" >
        </activity>
    </application>

</manifest>
